/* Quick Test of Open-SAS Functionality */

/* Create sample data */
data work.quick_test;
    input id name $ age salary department $;
    datalines;
1 Alice 30 60000 Sales
2 Bob 35 75000 IT
3 Charlie 28 55000 HR
4 Diana 32 70000 Sales
5 Edward 40 80000 IT
;
run;

/* Display the data */
proc print data=work.quick_test;
run;

/* Calculate statistics */
proc means data=work.quick_test;
    var age salary;
run;

/* Frequency analysis */
proc freq data=work.quick_test;
    tables department;
run;

/* Create high earners */
%let min_salary = 70000;

data work.high_earners;
    set work.quick_test;
    where salary >= &min_salary;
run;

/* Display high earners */
proc print data=work.high_earners;
run;
